div
  h1 ASelect
  a-select(
    v-model="model"
    :data="data"
    key-id="id"
    key-label="label"
    label="Test label"
    type="select"
    :search="true"
    key-group="group"
    sort-order-group="desc"
  )
  div model: {{ model }}
  a-select(
    v-model="model"
    :data="data"
    key-id="id"
    :key-label-callback="getLabel"
    label="Test label callback"
    type="select"
    :search="true"
    sort-order="desc"
  )

  br
  a-select(
    v-model="modelArr"
    :data="data"
    key-id="id"
    key-label="label"
    label="Test label multiselect"
    type="multiselect"
    :search="true"
    :is-select-all="true"
    :is-deselect-all="true"
  )
  div modelArr: {{ modelArr }}

  br
  a-select(
    v-model="modelArr2"
    :data="data"
    key-id="id"
    key-label="label"
    label="Test label multiselect2"
    type="multiselect"
    :search="true"
    :is-select-all="true"
    :is-deselect-all="true"
    :is-selection-closeable="false"
  )
  div modelArr2: {{ modelArr2 }}
