{
  "name": "aloha-vue",
  "version": "2.40.0",
  "description": "Aloha-vue is a JavaScript library that provides a wide range of accessible components and directives for Vue.js. Accessibility is of paramount importance to us, and we have designed all our components to meet accessibility compliance criteria. This library is a valuable tool for frontend developers and has already been used in three projects, including two large-scale ones",
  "keywords": [
    "accessibility compliance criteria",
    "accessible",
    "Aloha-vue",
    "components",
    "directives",
    "frontend developers",
    "JavaScript library",
    "large-scale projects",
    "utility",
    "Vue.js"
  ],
  "homepage": "https://github.com/ilia-brykin/aloha/#README.md",
  "bugs": {
    "url": "https://github.com/ilia-brykin/aloha/issues",
    "email": "aloha.vue@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Ilia Brykin",
    "email": "brykin.ilia@gmail.com"
  },
  "contributors": [
    {
      "name": "Emilija Zdilar"
    },
    {
      "name": "Hafedh Souissi"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ilia-brykin/aloha.git"
  },
  "scripts": {
    "build-lib": "vite build --config vite.lib.config.mjs",
    "build-styles": "sass scss/aloha-vue.scss dist/aloha-vue.css",
    "pub": "npm publish --scope=@npm",
    "test": "jest"
  },
  "main": "dist/aloha-vue.umd.js",
  "module": "dist/aloha-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/aloha-vue.es.js",
      "require": "./dist/aloha-vue.umd.js"
    },
    "./*": "./dist/*"
  },
  "dependencies": {
    "@floating-ui/vue": "1.1.5",
    "aloha-svg": "1.2.0",
    "axios": "1.8.3",
    "dompurify": "3.2.3",
    "fecha": "4.2.3",
    "lodash-es": "4.17.21",
    "moment": "2.30.1",
    "tiny-emitter": "2.1.0",
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@babel/cli": "7.26.4",
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@eslint/js": "^9.17.0",
    "@jest/globals": "^29.7.0",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/test-utils": "2.4.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "29.7.0",
    "babel-plugin-module-resolver": "5.0.2",
    "eslint": "^9.19.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-group": "^2.0.2",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-vue-pug": "^0.6.2",
    "globals": "^15.14.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lodash": "4.17.21",
    "sass": "1.83.0",
    "vite": "^6.0.11"
  },
  "files": [
    "dist",
    "src",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json",
    "scss"
  ]
}
