div
  h1 ASelect
  a-select(
    v-model="model"
    :data="data"
    key-id="id"
    key-label="label"
    label="Test label"
    type="select"
    :search="true"
    key-group="group"
    sort-order-group="desc"
    :loading="true"
  )
  div model: {{ model }}
  a-select(
    v-model="model"
    :data="data"
    key-id="id"
    :key-label-callback="getLabel"
    label="Test label callback"
    type="select"
    :search="true"
    sort-order="desc"
  )

  br
  a-select(
    v-model="modelArr"
    :data="data"
    key-id="id"
    key-label="label"
    label="Test label multiselect"
    type="multiselect"
    :search="true"
    :is-select-all="true"
    :is-deselect-all="true"
    slot-name="test"
  )
    template(
      v-slot:test="{ label, labelFiltered }"
    )
      a-icon.a_mr_1(
        icon="Cog"
      )
      span(
        v-if="labelFiltered"
        v-html="labelFiltered"
      )
      span(
        v-else
        v-html="label"
      )
  div modelArr: {{ modelArr }}

  br
  a-select(
    v-model="modelArr2"
    :data="data"
    key-id="id"
    key-label="label"
    label="Test label multiselect2"
    type="multiselect"
    :search="true"
    :is-select-all="true"
    :is-deselect-all="true"
    :is-selection-closeable="false"
    slot-name="aloha"
  )
    template(
      v-slot:aloha="{ label, labelFiltered }"
    )
      a-icon.a_mr_1(
        icon="Cog"
      )
      span(
        v-if="labelFiltered"
        v-html="labelFiltered"
      )
      span(
        v-else
        v-html="label"
      )
  div modelArr2: {{ modelArr2 }}
